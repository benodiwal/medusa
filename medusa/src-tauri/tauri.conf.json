{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "medusa",
  "version": "0.2.1",
  "identifier": "com.sachin.medusa",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "plugins": {
    "sql": {
      "preload": ["sqlite:medusa.db"]
    }
  },
  "app": {
    "windows": [
      {
        "title": "medusa",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "macOS": {
      "signingIdentity": null,
      "providerShortName": null,
      "entitlements": null,
      "minimumSystemVersion": "10.13"
    }
  }
}
